<Window
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:MOME"
        xmlns:WindowsPresentation="clr-namespace:GMap.NET.WindowsPresentation;assembly=GMap.NET.WindowsPresentation" x:Class="MOME.MainWindow"
        mc:Ignorable="d"
        Title="Map of music events" Height="900" Width="1600">
    <Grid HorizontalAlignment="Right" Width="1592">
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="0*"/>
            <ColumnDefinition/>
        </Grid.ColumnDefinitions>
        <TextBlock HorizontalAlignment="Left" Height="99" Margin="10,10,0,0" TextWrapping="Wrap" Text="Map of music events" VerticalAlignment="Top" Width="448" FontSize="48" Grid.ColumnSpan="2"/>
        <WindowsPresentation:GMapControl x:Name="map" HorizontalAlignment="Left" Height="678" Margin="10,159,0,0" VerticalAlignment="Top" Width="1174" Loaded="map_Loaded" Grid.ColumnSpan="2"/>
        <Button x:Name="start" Content="Start" HorizontalAlignment="Left" Height="54" Margin="1250,159,0,0" VerticalAlignment="Top" Width="236" Click="start_Click" Grid.ColumnSpan="2"/>
    </Grid>
</Window>
